<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fall2018.csc2017.slidingtiles">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="fall2018.csc2017.slidingtiles.LaunchCentre">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fall2018.csc2017.slidingtiles.GameActivity"></activity>
        <activity android:name="fall2018.csc2017.slidingtiles.GameSelection"></activity>
        <activity android:name="fall2018.csc2017.slidingtiles.StartingActivity"></activity>
        <activity android:name="fall2018.csc2017.slidingtiles.ScoreBoard"></activity>
        <service android:name=".ImageServiceIntent"></service>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>